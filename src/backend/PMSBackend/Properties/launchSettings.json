{
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iisExpress": {
            "applicationUrl": "http://localhost:7000;https://localhost:5001;https://192.168.40.112:8443",
            "sslPort": 5001
        }
    },
    "profiles": {
        //"http": {
        //    "commandName": "Project",
        //    "launchBrowser": true,
        //    "launchUrl": "swagger",
        //    "environmentVariables": {
        //        "ASPNETCORE_ENVIRONMENT": "Development"
        //    },
        //    "dotnetRunMessages": true,
        //    "applicationUrl": "http://localhost:7000"
        //},
        "PMSAPI-Development": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "applicationUrl": "http://localhost:7000;",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }           
        },
        "PMSAPI-Production": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "applicationUrl": "https://localhost:5001;https://192.168.40.112:8443",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Production"
            }           
        },
        "Container (Dockerfile)": {
            "commandName": "Docker",
            "launchBrowser": true,
            "applicationUrl": "http://0.0.0.0:7000",
            "environmentVariables": {
                "ASPNETCORE_HTTPS_PORTS": "8081",
                "ASPNETCORE_HTTP_PORTS": "8080"
            },
            "publishAllPorts": true,
            "useSSL": false
        }
    }
}


